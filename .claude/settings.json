{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "hooks": {
    "PostToolUse": [
      {
        "matcher": "Edit|Write",
        "hooks": [
          {
            "type": "command",
            "command": "\"$CLAUDE_PROJECT_DIR\"/.claude/hooks/doc-watcher-trigger.sh",
            "timeout": 5
          }
        ]
      }
    ]
  },
  "extraKnownMarketplaces": {
    "solsdev-marketplace": {
      "source": {
        "source": "github",
        "repo": "Neno73/solsdev"
      }
    }
  },
  "enabledPlugins": {
    "solsdev": true
  }
}
